	<typeAliases>
		<package name="com.jd.seed.dictionary.domain" />
		<package name="com.jd.seed.authority.domain" />
		<package name="com.jd.seed.organization.domain" />
	</typeAliases>
	<plugins>
		<plugin interceptor="com.github.pagehelper.PageInterceptor">
		</plugin>
	</plugins>
</configuration>